{
	"description": "Wrapper package for the VS Code File Downloader extension",
	"devDependencies": {
		"@types/node": "20.11.30",
		"@types/vscode": "1.87.0",
		"lodash": "4.17.21",
		"typescript": "5.4.2",
		"typescript-esbuild": "0.4.5"
	},
	"files": [
		"out/*",
		"CODE_OF_CONDUCT.md",
		"LICENSE.md",
		"README.md",
